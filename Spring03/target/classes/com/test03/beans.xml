<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="you" class="com.test03.Developer">
		<constructor-arg><value>정신형</value></constructor-arg>
		<constructor-arg value="500"></constructor-arg>
		<property name="dept" value="개발"></property>
	</bean>

	<bean id="kang" class="com.test03.Engineer">
		<constructor-arg value="강남스타일"/>
		<constructor-arg value="50000000"/>
		<property name="dept" value="기술"/>
	</bean>
	
</beans>
